<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Box background -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#335E84B5" />
            <corners android:radius="8dp" />
            <stroke
                android:width="2dp"
                android:color="#9BBEE0" />
        </shape>
    </item>
    <!-- White token circle centered -->
    <item
        android:left="14dp"
        android:top="14dp"
        android:right="14dp"
        android:bottom="14dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>
